@{
  Layout= "_Layout";
}
@using Bakery.Models;

<div class="container">
  <h4>Orders:</h4>
</div>

@if (@Model.Count == 0)
{
  <p>No orders have been placed, sorry!</p>
}

@foreach (Order order in Model)
{
  <h4>@order.OrderTitle</h4>
} 


<p><a href="/vendors">Vendors Page</a></p>
<p><a href="/">Phone Home!</a></p> 
